{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Terminal (integrated)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Attach (Remote Debug)",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost"
    },
  {
    "name": "Flask",
    "type": "python",
    "request": "launch",
    "module": "flask",
    "env": {
      "FLASK_APP": "app.py",
      "FLASK_ENV": "development",
      "FLASK_DEBUG": "0"
    },
    "args": [
      "run",
      "--no-debugger",
      "--no-reload"
    ],
    "jinja": true
  },
    // {
    //   "type": "node",
    //   "request": "launch",
    //   "name": "Launch Program",
    //   "program": "${workspaceFolder}\\server\\bin\\www"
    // }
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector",
    },
  ]
}