{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Terminal flask",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/server/main.py",
      "console": "integratedTerminal"
    },

    {
      "name": "Python: Terminal (integrated)",
      "type": "python",
      "request": "launch",
      "program": "dev_appserver.py will.yaml",
      "console": "integratedTerminal",
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "/Users/will/Projects/ascee/credentials/ascee-recruit-95a50ceae351.json",
        "CLOUDSDK_PYTHON": "python2.7"
      }
    },
    {
      "name": "Attach (Remote Debug)",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost"
    },
  {
    "name": "Flask",
    "type": "python",
    "request": "launch",
    "module": "flask",
    "env": {
      "FLASK_APP": "app.py",
      "FLASK_ENV": "development",
      "FLASK_DEBUG": "0"
    },
    "args": [
      "run",
      "--no-debugger",
      "--no-reload"
    ],
  },
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector",
    },
  ]
}